{
    "name": "dandysi/laravel-openswoole-stats",
    "description": "A laravel package to expose Open Swoole metrics to a scraping service like Prometheus",
    "keywords": ["php", "laravel", "openswoole", "metrics", "prometheus"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "role": "Developer",
            "name": "Simon Adams"
        }
    ],
    "autoload": {
        "psr-4": {
            "Dandysi\\Laravel\\OpenSwooleStats\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dandysi\\Laravel\\OpenSwooleStats\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0",
        "illuminate/console": "^11.0",
        "ext-openswoole": ">=22.0",
        "openswoole/core" : "^22.0.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.18|^9.0",
        "phpunit/phpunit": "^10.5",
        "friendsofphp/php-cs-fixer": "^3.42",
        "phpstan/phpstan": "^1.10"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Dandysi\\Laravel\\OpenSwooleStats\\OpenSwooleStatsServiceProvider"
            ]
        }
    }
}
